#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/haimac/Automation-SwagLabsDemo/node_modules/@cucumber/gherkin-utils/bin/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules/@cucumber/gherkin-utils/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules/@cucumber/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules:/Users/haimac/node_modules:/Users/node_modules:/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/haimac/Automation-SwagLabsDemo/node_modules/@cucumber/gherkin-utils/bin/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules/@cucumber/gherkin-utils/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules/@cucumber/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules:/Users/haimac/node_modules:/Users/node_modules:/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@cucumber/gherkin-utils/bin/gherkin-utils" "$@"
else
  exec node  "$basedir/../@cucumber/gherkin-utils/bin/gherkin-utils" "$@"
fi
