#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/haimac/Automation-SwagLabsDemo/node_modules/.pnpm/gherkin@5.1.0/node_modules/gherkin/bin/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules/.pnpm/gherkin@5.1.0/node_modules/gherkin/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules/.pnpm/gherkin@5.1.0/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/haimac/Automation-SwagLabsDemo/node_modules/.pnpm/gherkin@5.1.0/node_modules/gherkin/bin/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules/.pnpm/gherkin@5.1.0/node_modules/gherkin/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules/.pnpm/gherkin@5.1.0/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../gherkin/bin/gherkin" "$@"
else
  exec node  "$basedir/../gherkin/bin/gherkin" "$@"
fi
