#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/haimac/Automation-SwagLabsDemo/node_modules/.pnpm/cucumber@4.2.1/node_modules/cucumber/bin/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules/.pnpm/cucumber@4.2.1/node_modules/cucumber/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules/.pnpm/cucumber@4.2.1/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/haimac/Automation-SwagLabsDemo/node_modules/.pnpm/cucumber@4.2.1/node_modules/cucumber/bin/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules/.pnpm/cucumber@4.2.1/node_modules/cucumber/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules/.pnpm/cucumber@4.2.1/node_modules:/Users/haimac/Automation-SwagLabsDemo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../cucumber@4.2.1/node_modules/cucumber/bin/cucumber-js" "$@"
else
  exec node  "$basedir/../../../../../cucumber@4.2.1/node_modules/cucumber/bin/cucumber-js" "$@"
fi
